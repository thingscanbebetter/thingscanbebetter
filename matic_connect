// URL for endpoint
 var endpointURL = 'https://rpc-mainnet.matic.network'; // matic
// var endpointURL = 'https://mainnet.infura.io/v3/7dc960db53eb46a3abc7a40ee3de7d3d'; // ETH

// Address for Prize Pool Contract
var prizePoolAddress = "0x27DF5C6dcd360f372e23d5e63645eC0072D0C098"; // matic
// var prizePoolAddress = "0x3D9946190907aDa8b70381b25c71eB9adf5f9B7b"; // ETH

// ABI for Prize Pool Contract
const prizePoolABI = [ "function owners() external view returns (address)"
 ];

	var customHttpProvider = new ethers.providers.JsonRpcProvider(endpointURL);
    
  customHttpProvider.getBlockNumber().then((result) => {
 document.write("Current block number: " + result); });


async function run() {

    const prizePoolContract = new ethers.Contract(prizePoolAddress, prizePoolABI, customHttpProvider);
   
    let owners = await prizePoolContract.owners();
   owners = owners.toString();
    document.write("<br>owners:" + owners); 

  }
  run();
    
